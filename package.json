{
  "name": "doubloons",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "require": "./dist/index.cjs"
      }
    },
    "./currencies": {
      "import": {
        "types": "./dist/currencies.d.ts",
        "import": "./dist/currencies.js"
      },
      "require": {
        "types": "./dist/currencies.d.cts",
        "require": "./dist/currencies.cjs"
      }
    }
  },
  "scripts": {
    "btsup": "tsup --entry.index src/index.ts --entry.currencies src/Currency.ts --format cjs,esm --dts --clean",
    "build": "tsx bin/build.ts",
    "lint": "pnpm eslint .",
    "format": "pnpm prettier --write ./src",
    "attw": "npx @arethetypeswrong/cli --pack . --profile node16 --verbose",
    "test": "vitest run --coverage && npx istanbul-badges-readme",
    "test:ui": "vitest --ui",
    "gen-docs": "typedoc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.6.5+sha512.cdf928fca20832cd59ec53826492b7dc25dc524d4370b6b4adbf65803d32efaa6c1c88147c0ae4e8d579a6c9eec715757b50d4fa35eea179d868eada4ed043af",
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.4",
    "@eslint/js": "^9.22.0",
    "@types/node": "^22.13.11",
    "@vitest/coverage-istanbul": "3.0.9",
    "@vitest/coverage-v8": "3.0.9",
    "@vitest/ui": "^3.0.9",
    "eslint": "^9.22.0",
    "istanbul-badges-readme": "^1.9.0",
    "jiti": "^2.4.2",
    "prettier": "^3.5.3",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typedoc": "^0.28.1",
    "typedoc-plugin-markdown": "^4.5.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0",
    "vitest": "^3.0.9"
  },
  "dependencies": {
    "decimal.js": "^10.5.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
